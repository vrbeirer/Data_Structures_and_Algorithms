    "def move_zeroes(arr):\n",
    "    non_zero_index = 0\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i] != 0:\n",
    "            arr[i], arr[non_zero_index] = arr[non_zero_index], arr[i]\n",
    "            non_zero_index += 1\n",
    "    return arr\n",
    "\n",
    "# Test\n",
    "arr = [0, 1, 0, 3, 12]\n",
    "print(move_zeroes(arr))  "
   ]
