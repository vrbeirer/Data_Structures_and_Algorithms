   "source": [
    "N = int(input(\"Enter number of elements: \"))\n",
    "arr = [int(input()) for _ in range(N)]\n",
    "mean = sum(arr) / N\n",
    "variance = sum((x - mean) ** 2 for x in arr) / N\n",
    "std_dev = variance ** 0.5\n",
    "print(\"Standard Deviation:\", std_dev)"
   ]
