   ],
   "source": [
    "def gcd_recursive(m, n):\n",
    "    if n == 0:\n",
    "        return m\n",
    "    else:\n",
    "        return gcd_recursive(n, m % n)\n",
    "\n",
    "# Example usage:\n",
    "result = gcd_recursive(14, 63)\n",
    "print(f\"The GCD of 14 and 63 is: {result}\")  # Output: The GCD of 14 and 63 is: 7"
   ]
